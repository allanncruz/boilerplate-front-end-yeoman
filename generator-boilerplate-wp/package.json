{
  "name": "generator-boilerplate-wp",
  "version": "1.1.3",
  "description": "WordPress theme boilerplate generator",
  "homepage": "http://allancruz.com.br",
  "author": {
    "name": "Allan Cruz",
    "email": "contato@allancruz.com.br",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "generator",
    "boilerplate",
    "wordpress",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-xo": "^0.25.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^1.3.1",
    "jest": "^23.5.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "https://github.com/allanncruz/boilerplate-front-end-yeoman.git",
  "license": "MIT"
}
